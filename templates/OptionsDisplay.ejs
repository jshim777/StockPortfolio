<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Stock Options Display</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      margin: 0;
      padding: 0;
      background-color: #f4f4f4;
    }
    
    .container {
      max-width: 800px;
      margin: 0 auto;
      padding: 20px;
    }
    
    h1 {
      text-align: center;
      margin-bottom: 30px;
    }
    
    table {
      width: 100%;
      border-collapse: collapse;
      background-color: white;
      border-radius: 5px;
      box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
    }
    
    th, td {
      padding: 12px 16px;
      text-align: left;
      border-bottom: 1px solid #f1f1f1;
    }
    
    th {
      background-color: #f5f5f5;
      font-weight: bold;
    }
    
    tr:last-child td {
      border-bottom: none;
    }
    
    a {
      color: #007bff;
      text-decoration: underline;
    }
    
    a:hover {
      color: #0056b3;
    }
  </style>
</head>
<body>
  <div class="container">
    <h1>Stock Options Display</h1>
    
    <table>
      <thead>
        <tr>
          <th>Exchange Display</th>
          <th>Exchange</th>
          <th>Name</th>
          <th>Symbol</th>
          <th>Index</th>
          <th>Quote Type</th>
          <th>Type Display</th>
          <th>Score</th>
          <th>Sector</th>
          <th>Industry</th>
        </tr>
      </thead>
      <tbody>
        <% stockData.quotes.forEach(stock => { %>
          <tr>
            <td><%= stock.exchDisp %></td>
            <td><%= stock.exchange %></td>
            <td><%= stock.shortname %></td>
            <td><%= stock.symbol %></td>
            <td><%= stock.index %></td>
            <td><%= stock.quoteType %></td>
            <td><%= stock.typeDisp %></td>
            <td><%= stock.score %></td>
            <td><%= stock.sectorDisp %></td>
            <td><%= stock.industryDisp %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
    
    <hr>
    
    <a href="/">HOME</a>
  </div>
</body>
</html>